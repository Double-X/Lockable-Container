package doublex.lib.locks.exceptions;

/**
 *
 * @author DoubleX
 */
public final class ResetCountFailException extends Exception {

    private static final long serialVersionUID = 1L;
    private static final String MESSAGE = "The counter can't be reset!";

    public ResetCountFailException() {
        super(MESSAGE);
    }

    public ResetCountFailException(final Throwable cause) {
        super(MESSAGE, cause);
    }

    public ResetCountFailException(final String message) {
        super(message);
    }

    public ResetCountFailException(final String message, final Throwable cause) {
        super(message, cause);
    }

}

package doublex.lib.locks.exceptions;

/**
 *
 * @author DoubleX
 */
public final class MaxKeyMismatchCountReachedException extends Exception {

    private static final long serialVersionUID = 1L;
    private static final String MESSAGE = 
            "The maxiumm key mismatch count's reached!";

    public MaxKeyMismatchCountReachedException() {
        super(MESSAGE);
    }

    public MaxKeyMismatchCountReachedException(final Throwable cause) {
        super(MESSAGE, cause);
    }

    public MaxKeyMismatchCountReachedException(final String message) {
        super(message);
    }

    public MaxKeyMismatchCountReachedException(
            final String message, final Throwable cause) {
        super(message, cause);
    }

}
